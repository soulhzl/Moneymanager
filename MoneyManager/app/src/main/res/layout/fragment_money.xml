<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/ll_bg"
    android:focusable="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:baselineAligned="false">

            <FrameLayout
                android:id="@+id/jz_main_zhichu_fl"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="20dp"
                android:layout_weight="1"
                android:background="@mipmap/t_bg">

                <ImageView
                    android:id="@+id/jz_tab1_ivzhichu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:contentDescription="@string/hello" />

                <TextView
                    android:id="@+id/jz_tab1_btzhichu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="25dp"
                    android:text="@string/jz_zhichu"
                    android:textColor="#000000"
                    android:textSize="18dp" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/jz_main_shouru_fl"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                android:background="@mipmap/t_bg">

                <ImageView
                    android:id="@+id/jz_tab1_ivshouru"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:contentDescription="@string/hello" />

                <TextView
                    android:id="@+id/jz_tab1_shouru"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="25dp"
                    android:text="@string/jz_shouru"
                    android:textColor="#000000"
                    android:textSize="18dp" />
            </FrameLayout>
        </LinearLayout>

        <FrameLayout
            android:id="@+id/jz_fl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/jz_ll_zhichu"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"

                        android:background="@mipmap/t_bg2"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="50dp"
                                android:text="@string/jz_benzhou_zhichu"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#000000"
                                android:layout_marginTop="5dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text=":"
                                android:textColor="#000000"
                                android:layout_marginTop="5dp" />

                            <TextView
                                android:id="@+id/jz_main_zhichu_week_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text="@string/jz_0"
                                android:textColor="#ff0000"
                                android:layout_marginTop="5dp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="50dp"
                                android:text="@string/jz_benyue_zhichu"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#000000" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text=":"
                                android:textColor="#000000" />

                            <TextView
                                android:id="@+id/jz_main_zhichu_month_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text="@string/jz_0"
                                android:textColor="#ff0000" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="50dp"
                                android:text="@string/jz_benyue_shouru"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#000000"
                                android:layout_marginBottom="5dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text=":"
                                android:textColor="#000000"
                                android:layout_marginBottom="5dp" />

                            <TextView
                                android:id="@+id/jz_main_zhichu_shouru_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text="@string/jz_0"
                                android:textColor="#ff0000"
                                android:layout_marginBottom="5dp" />
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2dip"
                        android:background="@mipmap/jz_bg_bottom"
                        android:contentDescription="@string/app_name" />

                    <RelativeLayout
                        android:id="@+id/jz_main_zhichu_pic_shang_rl"
                        android:layout_width="match_parent"
                        android:layout_height="130dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="120dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="10dp"
                        android:gravity="center"></RelativeLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2dip"
                        android:background="@mipmap/jz_bg_bottom"
                        android:contentDescription="@string/app_name" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:background="@mipmap/t_bg2"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="50dp"
                                android:text="@string/jz_benyueyusuan"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#000000"
                                android:layout_marginTop="5dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text=":"
                                android:textColor="#000000"
                                android:layout_marginTop="5dp" />

                            <TextView
                                android:id="@+id/jz_main_zhichu_yusuan_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text="@string/jz_0"
                                android:textColor="#ff0000"
                                android:layout_marginTop="5dp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="50dp"
                                android:text="@string/jz_yusuanshengyu"
                                android:textAppearance="?android:attr/textAppearanceMedium"
                                android:textColor="#000000"
                                android:layout_marginBottom="5dp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text=":"
                                android:textColor="#000000"
                                android:layout_marginBottom="5dp" />

                            <TextView
                                android:id="@+id/jz_main_zhichu_yusuanyue_text"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="60dp"
                                android:text="@string/jz_0"
                                android:textColor="#ff0000"
                                android:layout_marginBottom="5dp" />
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2dip"
                        android:background="@mipmap/jz_bg_bottom"
                        android:contentDescription="@string/app_name" />

                    <RelativeLayout
                        android:id="@+id/jz_main_zhichu_pic_xia_rl"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:layout_marginLeft="90dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginTop="10dp">

                    </RelativeLayout>

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="2dip"
                        android:background="@mipmap/jz_bg_bottom"
                        android:contentDescription="@string/app_name" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/jz_ll_shouru"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"

                    android:background="@mipmap/t_bg2"
                    android:layout_marginRight="10dp"
                    android:layout_marginLeft="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="50dp"
                            android:text="@string/jz_shouru_bennian"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000"
                            android:layout_marginTop="5dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="60dp"
                            android:text=":"
                            android:textColor="#000000"
                            android:layout_marginTop="5dp" />

                        <TextView
                            android:id="@+id/jz_main_shouru_year_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="60dp"
                            android:text="@string/jz_0"
                            android:textColor="#ff0000"
                            android:layout_marginTop="5dp" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="50dp"
                            android:text="@string/jz_shouru_benyue"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="60dp"
                            android:text=":"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/jz_main_shouru_month_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="60dp"
                            android:text="@string/jz_0"
                            android:textColor="#ff0000" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="50dp"
                            android:text="@string/jz_shouru_jintian"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#000000"
                            android:layout_marginBottom="5dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="60dp"
                            android:text=":"
                            android:textColor="#000000"
                            android:layout_marginBottom="5dp" />

                        <TextView
                            android:id="@+id/jz_main_shouru_day_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="60dp"
                            android:text="@string/jz_0"
                            android:textColor="#ff0000"
                            android:layout_marginBottom="5dp" />

                    </LinearLayout>
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2dip"
                    android:background="@mipmap/jz_bg_bottom"
                    android:contentDescription="@string/app_name" />

                <RelativeLayout
                    android:id="@+id/jz_main_shouru_pic_shang_rl"
                    android:layout_width="match_parent"
                    android:layout_height="130dp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="85dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="10dp">

                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="2dip"
                    android:background="@mipmap/jz_bg_bottom"
                    android:contentDescription="@string/app_name" />

            </LinearLayout>
        </FrameLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_gravity="bottom"
        android:background="@mipmap/money_bg"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <Button
                android:id="@+id/jz_main_bt_add"
                style="@style/jz_mian_button_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/jz_bottombt_selector"
                android:drawableTop="@mipmap/jz_main_bt_add"
                android:text="@string/jz_main_add" />

            <Button
                android:id="@+id/jz_main_bt_yusuan"
                style="@style/jz_mian_button_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/jz_bottombt_selector"
                android:drawableTop="@mipmap/jz_main_bt_yusuan"
                android:text="@string/jz_main_yusuan" />

            <ImageView
                android:id="@+id/jz_mian_menuiv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:contentDescription="@string/hello"
                android:src="@mipmap/jz_main_more" />

            <Button
                android:id="@+id/jz_main_bt_mingxi"
                style="@style/jz_mian_button_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/jz_bottombt_selector"
                android:drawableTop="@mipmap/jz_main_bt_mingxi"
                android:text="@string/jz_main_mingxi" />

            <Button
                android:id="@+id/jz_main_bt_baobiao"
                style="@style/jz_mian_button_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@drawable/jz_bottombt_selector"
                android:drawableTop="@mipmap/jz_main_bt_baobiao"
                android:text="@string/jz_main_baobiao" />
        </LinearLayout>
    </LinearLayout>

</FrameLayout>